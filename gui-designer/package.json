    "mocha": "2.x.x"
  },
  "dependencies": {
    "@babel/runtime": "^7.1.5",
    "baconipsum": "^0.1.2",
    "bcrypt": "^0.8.7",
    "classnames": "^2.2.3",
    "domready": "^1.0.8",
    "fontawesome": "^4.7.2",
    "mantra-core": "^1.2.0",
    "meteor-auth": "^0.2.0",
    "meteor-node-stubs": "^0.2.1",
    "react": "^16.6.1",
    "react-clock": "^2.3.0",
    "react-dnd": "^2.1.4",
    "react-dnd-html5-backend": "^2.1.2",
    "react-dom": "^16.6.1",
    "react-fontawesome": "^1.6.1",
    "react-joyride": "^1.3.6",
    "react-json-tree": "^0.7.3",
    "react-jss": "^8.6.1",
    "react-komposer": "1.x.x",
    "react-modal": "^1.2.1",
